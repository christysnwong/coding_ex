{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Solicitor"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                131000
            ]
        },
        "equity": {
            "type": "string",
            "default": "null",
            "title": "The equity Schema",
            "examples": [
                "0.034"
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "wiggins-frederick-boyer"
            ]
        }
    },
    "additionalProperties": false,
    "examples": [{
        "title": "Solicitor",
        "salary": 131000,
        "equity": "0.034",
        "companyHandle": "wiggins-frederick-boyer"
    }]
}